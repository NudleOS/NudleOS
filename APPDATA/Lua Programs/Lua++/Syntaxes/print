function UTF8
print(UTF8:"Hello World!")
function UTF8:len(str)
    return #str
end

function UTF8:sub(str, i, j)
    return string.sub(str, i, j)
end

function UTF8:char(str, i)
    return string.char(str, i)
end

function UTF8:byte(str, i)
    return string.byte(str, i)
end

function UTF8:format(str, ...)
    return string.format(str, ...)
end

function UTF8:upper(str)
    return string.upper(str)
end

function UTF8:lower(str)
    return string.lower(str)
end

function UTF8:find(str, pattern, init, plain)
    return string.find(str, pattern, init, plain)
end

function UTF8:match(str, pattern, init)
    return string.match(str, pattern, init)
end

function UTF8:gsub(str, pattern, repl, n)
    return string.gsub(str, pattern, repl, n)
end

function UTF8:reverse(str)
    return string.reverse(str)
end

function UTF8:insert(str, pos, ...)
    return string.insert(str, pos, ...)
end

function UTF8:remove(str, pos)
    return string.remove(str, pos)
end

function UTF8:concat(str, sep)
    return table.concat(str, sep)
end

function UTF8:remove_utf8(str)
    local result = ""
    for i = 1, #str do
        local byte = string.byte(str, i)
        if byte < 128 then
            result = result .. string.char(byte)
        end
    end
    return result
end

function UTF8:print(str)
    print(self:remove_utf8(str))
end

print(UTF8:"Hello World!")
return UTF8
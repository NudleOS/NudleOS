--The script behind the new import syntax in Lua++
function import(module)
    local function ()
    end
end
import = function(module)
    local f, err = loadfile(module)
    if f then
        f()
    else
        print(err)
    end
end

--Example Usage:
--import(MyModule)
--Below is a program that uses import
import("Lua3D")
root = Lua3D.window
root 1200, 800
root.Lua3D.cube
--That code creates a cube above. Below is the values for size and location.

local size = 10
local location = {x=0, y=0, z=0}
Lua3D.cube(size, location)